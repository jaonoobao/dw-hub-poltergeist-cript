Lighting.Ambient = Color3.fromRGB(255, 255, 255)  -- Configura el color ambiental para hacerlo m谩s brillante
Lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)  -- Establece la luz ambiental exterior a blanco brillante

-- Si hay alguna luz espec铆fica que quieras cambiar, puedes ajustar la intensidad de las luces globalmente
Lighting.FogEnd = 100000  -- Desactivar la niebla para eliminar el efecto de oscuridad a larga distancia

-- Si se utiliza un "TimeOfDay" espec铆fico, puedes configurarlo para que sea brillante durante todo el d铆a
