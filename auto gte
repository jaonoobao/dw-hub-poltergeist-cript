                [3] = game:GetService("Players").LocalPlayer.Character
            }

            game:GetService("ReplicatedStorage").Events.AbilityEvent:InvokeServer(unpack(args))
        end

        -- Llamar a la funci贸n al presionar el bot贸n
        invokeAbilityEvent()
